{
  "expo": {
    "name": "ft_hangouts",
    "slug": "ft_hangouts",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/images/42.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.ft-hangouts"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/42.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.ft_hangouts",
      "permissions": [
        "RECEIVE_SMS",
        "READ_SMS",
        "SEND_SMS"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/42.png"
    },
    "plugins": [
      "expo-router",
      "./withSmsReceiver.js",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/42.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-localization",
      "expo-sqlite"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
